@model List<ModelClass>
@{
    ViewData["Title"] = "Home Page";
}

<!DOCTYPE html>
<html>
<head>
    
</head>
<body>
    <h2>Contacts</h2>

    @*<p1>@ViewBag.Message</p1>*@

    <table class="table table-bordered" cellpadding="0" cellspacing="0">
        <tr>
            <th>Id</th>
            <th>Contact Name</th>
        </tr>
        @foreach (ModelClass name in Model)
        {
            <tr>
                <td>@name.id</td>
                <td>@name.contact_name</td>
            </tr>
        }
    </table>

</body>
</html>